{
  "version": 2,
  "projects": {
    "store": {
      "root": "apps/store",
      "sourceRoot": "apps/store",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "root": "apps/store",
            "outputPath": "dist/apps/store"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "store:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "store:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "store:build:production"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/store"],
          "options": {
            "jestConfig": "apps/store/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/store/**/*.{ts,tsx,js,jsx}"]
          }
        }
      }
    },
    "store-e2e": {
      "root": "apps/store-e2e",
      "sourceRoot": "apps/store-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/store-e2e/cypress.json",
            "tsConfig": "apps/store-e2e/tsconfig.e2e.json",
            "devServerTarget": "store:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "store:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/store-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "tailwind-ui": {
      "root": "libs/tailwind-ui",
      "sourceRoot": "libs/tailwind-ui/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/tailwind-ui",
            "tsConfig": "libs/tailwind-ui/tsconfig.lib.json",
            "project": "libs/tailwind-ui/package.json",
            "entryFile": "libs/tailwind-ui/src/index.ts",
            "external": ["react/jsx-runtime"],
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "libs/tailwind-ui/README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/tailwind-ui/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/tailwind-ui"],
          "options": {
            "jestConfig": "libs/tailwind-ui/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/next"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "babel": true
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "css",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "store"
}
